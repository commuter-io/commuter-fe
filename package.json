		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^5.5.0",
		"react-query": "^3.39.3",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4.1.12",
		"@types/node": "^20",
